import java.util.Locale;

public class Main {

	public static void main(String[] args) {
		
		double x = 10.35784;
		
		String product1 = "Computer";
		String product2 = "Office desk";
		
		int age = 30;
		int code = 5290;
		char gender = 'f';
		
		double price1 = 2100.0;
		double price2 = 650.50;
		double measure = 53.234567;
		
		System.out.printf("%nProducts: %n %s, which price is $ %f ", product1, price1);
		System.out.printf("%nProducts: %n %s, which price is $ %f", product2, price2);
		
		System.out.printf("%n%n Record: %d years old, code %d and gender: %s %n", age, code, gender);
		
		System.out.printf("%n Measue with eight decimal places: %f ", measure);
		System.out.printf("%n Rouded (three decimal places): %.3f ", measure);
		Locale.setDefault(Locale.US);
		System.out.printf("%n US decimal point: {measure us} %.3f ", measure);

	}

}
