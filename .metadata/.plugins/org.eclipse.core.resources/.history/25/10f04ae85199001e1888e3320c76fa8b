
public class Main {

	public static void main(String[] args) {
		
		double x = 10.35784;
		
		String product1 = "Computer";
		String product2 = "Office desk";
		
		int age = 30;
		int code = 5290;
		char gender = 'f';
		
		double price1 = 2100.0;
		double price2 = 650.50;
		double measure = 53.234567;
		
		System.out.printf("Products: %n %s, which price is $ %f %n", product1, price1);
		System.out.printf("Products: %n %s, which price is $ %f %n", product2, price2);
		
		System.out.printf("%n %n Record: 30 years old, code %d and gender: %s %n", code, gender);
		
		System.out.printf("%n %n Measue with eight decimal places: {measure}  %n Rouded (three decimal places): {measure} %n US decimal point: {measure us}", args)
		
		System.out.printf("%.2f%n", x);

	}

}
